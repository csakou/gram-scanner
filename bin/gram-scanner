#!/usr/bin/env python
import os
import click
from gramscanner.gramscanner import GramScanner

@click.command()
@click.argument('GramWebUser', envvar='GramWebUser')
@click.argument('GramWebPass', envvar='GramWebPass')
def main(gramwebuser, gramwebpass):
  gs = GramScanner(gramwebuser, gramwebpass)
  data = gs.get_grades()
  gs.print_grades(data)
    
if(__name__ == "__main__"):
  main()
